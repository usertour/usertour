{
  "name": "@usertour/server",
  "version": "0.1.1",
  "description": "The Server Api For Usertour App",
  "homepage": "https://www.usertour.io",
  "author": "Usertour, Inc.",
  "license": "AGPL-3.0",
  "private": true,
  "keywords": ["usertour", "onboarding", "tour", "tooltip"],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "prisma db seed",
    "prepare": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@aws-sdk/client-s3": "^3.441.0",
    "@aws-sdk/credential-providers": "^3.441.0",
    "@aws-sdk/s3-request-presigner": "^3.441.0",
    "@aws-sdk/util-format-url": "^3.433.0",
    "@devoxa/prisma-relay-cursor-connection": "3.1.1",
    "@golevelup/nestjs-stripe": "~0.8.2",
    "@nestjs/apollo": "12.1.0",
    "@nestjs/axios": "3.0.2",
    "@nestjs/bullmq": "~10.2.3",
    "@nestjs/common": "10.3.7",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.7",
    "@nestjs/graphql": "12.1.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.7",
    "@nestjs/platform-socket.io": "10.3.7",
    "@nestjs/platform-ws": "10.3.7",
    "@nestjs/swagger": "7.3.1",
    "@nestjs/websockets": "10.3.7",
    "@opentelemetry/api": "^1.9.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "5.12.1",
    "@smithy/hash-node": "^2.0.12",
    "@smithy/protocol-http": "^3.0.8",
    "@smithy/url-parser": "^2.0.12",
    "@socket.io/postgres-adapter": "^0.4.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/mjml": "^4.7.1",
    "argon2": "^0.41.1",
    "axios": "^1.6.2",
    "bcryptjs": "2.4.3",
    "bullmq": "^5.40.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.7",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "express-rate-limit": "^7.4.1",
    "graphql": "16.6.0",
    "graphql-scalars": "1.20.1",
    "graphql-subscriptions": "2.0.0",
    "graphql-type-json": "^0.3.2",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "mjml": "^4.14.1",
    "ms": "^2.1.3",
    "nestjs-pino": "^4.4.0",
    "nestjs-prisma": "0.23.0",
    "nodemailer": "^6.9.4",
    "passport": "0.6.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "4.0.1",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "socket.io": "^4.7.2",
    "stripe": "^18.0.0",
    "uuid": "^9.0.1",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.7",
    "@types/bcryptjs": "2.4.6",
    "@types/chance": "1.1.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/node": "^18.15.0",
    "@types/pg": "^8.11.10",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "chance": "1.1.11",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "pino-pretty": "^13.0.0",
    "prettier": "2.8.4",
    "prisma": "5.12.1",
    "prisma-dbml-generator": "0.12.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-morph": "^17.0.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": ["node_modules", "<rootDir>/../node_modules"]
  }
}
